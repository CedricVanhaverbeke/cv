{
  "name": "cedric-vanhaverbeke-cv",
  "private": true,
  "version": "0.1.0",
  "author": "Cedric Vanhaverbeke",
  "dependencies": {
    "a11y-react-emoji": "^1.1.2",
    "gatsby": "^2.29.1",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-offline": "^3.2.33",
    "gatsby-plugin-postcss": "^3.0.3",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.6.42",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-transformer-remark": "^2.16.1",
    "gatsby-transformer-sharp": "^2.5.17",
    "postcss": "^8.1.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-code-blocks": "^0.0.8",
    "react-console-emulator": "^5.0.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.1.0",
    "rehype-react": "^6.2.0",
    "tailwind": "^4.0.0"
  },
  "devDependencies": {
    "prettier": "2.1.2",
    "tailwindcss": "^1.9.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "npm run css:prod && gatsby build",
    "develop": "npm run css:dev && gatsby develop -p 3000",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "css:dev": "tailwind build ./src/theme/index.css -c ./tailwind.config.js -o ./src/theme/tailwind.css",
    "css:prod": "NODE_ENV=production yarn css:dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
